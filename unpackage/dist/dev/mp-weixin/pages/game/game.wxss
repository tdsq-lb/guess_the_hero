.game {
  position: absolute;
  background-image: url(https://tdsq.top/static/images/bg.jpg);
  background-color: black;
  background-repeat: no-repeat;
  background-size: 100%;
  width: 100%;
  height: 100%;
}
.game .game-position {
  width: 20%;
  height: 120rpx;
  position: absolute;
  left: 0;
  top: 40rpx;
}
.game .game-position .game-position-power {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 100%;
  height: 50%;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #131516;
  background-image: url(https://tdsq.top/static/images/enemy_economic_lose.9.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.game .game-position .game-position-times {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 100%;
  height: 50%;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #131516;
  background-image: url(https://tdsq.top/static/images/enemy_ecnomic_win.9.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.game .game-content {
  width: 94%;
  height: 80%;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  background-color: #FFFFFF;
  position: absolute;
  bottom: 0;
  left: 3%;
  padding: 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.game .game-content-header {
  width: 100%;
  height: 50rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.game .game-content-header .game-content-header-tips {
  width: 20%;
  height: 100%;
  text-align: center;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #ba9a6c;
  background-image: url(https://tdsq.top/static/images/tips.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.game .game-content-image {
  width: 100%;
  height: 320rpx;
  border: 1px solid #ccb089;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin: 20rpx auto;
  padding: 5rpx;
  position: relative;
}
.game .game-content-image .game-content-image-frame {
  width: 100%;
  height: 100%;
  border: 1px solid #ccb089;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask .item {
  width: 50%;
  height: 50%;
  background-color: #000;
  position: absolute;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask .left_top {
  left: 0;
  top: 0;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask .right_top {
  right: 0;
  top: 0;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask .left_bottom {
  left: 0;
  bottom: 0;
}
.game .game-content-image .game-content-image-frame .game-content-image-frame-Mask .right_bottom {
  right: 0;
  bottom: 0;
}
.game .game-content-audio {
  background-color: #555555;
}
.game .game-content-options {
  margin: 20rpx 0;
}

