.content {
  position: absolute;
  background-image: url(https://i.loli.net/2020/11/05/3B5wFKXIz6ibAtT.jpg);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
}
.content .main {
  position: absolute;
  width: 100%;
  left: 0;
  top: 50%;
}
.content .main .option {
  width: 468rpx;
  margin: 0 auto;
  padding: 10px 0;
}
.content .main .option .option_item {
  width: 100%;
  height: 35px;
  position: relative;
  margin: 0 0 16px;
  text-align: center;
}
.content .main .option .option_item input {
  width: 100%;
  height: 100%;
  background-color: rgba(255, 255, 255, 0.25);
  text-align: center;
  color: #ddd;
  line-height: 35px;
  font-size: 14px;
}
.content .main .option .option_item .option_item_txt {
  width: 100%;
  height: 100%;
  background-color: rgba(255, 255, 255, 0.25);
  text-align: center;
  color: #ddd;
  line-height: 35px;
  font-size: 14px;
}
.content .main .option .option_item .start {
  width: 234px;
  height: 46px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  display: inline-block;
  text-align: center;
  color: #fff;
  font-size: 18px;
  font-weight: 600;
  letter-spacing: 3px;
  line-height: 46px;
}
.content .uni-input-placeholder {
  color: #ddd;
}

