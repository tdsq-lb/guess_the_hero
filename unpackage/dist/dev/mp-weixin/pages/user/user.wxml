<view class="user"><image class="user-bg" src="https://tdsq.top/static/images/bg.jpg" mode="scaleToFill" lazy-load="{{true}}" data-event-opts="{{[['load',[['imageLoad',['$event']]]]]}}" bindload="__e"></image><view class="user-content"><image class="user-content-image" src="https://tdsq.top/static/images/bg-msg.png" mode="scaleToFill" lazy-load="{{true}}" data-event-opts="{{[['load',[['imageLoad',['$event']]]]]}}" bindload="__e"></image><view data-event-opts="{{[['tap',[['handleRemoveUser',['$event']]]]]}}" class="reset" bindtap="__e"><text class="iconfont icon-zhongxin"></text><text>重新绑定ID</text></view><view class="user-bg-content"><view class="avatar"><block wx:if="{{!islogin}}"><view class="avatar1"></view></block><block wx:if="{{islogin}}"><view class="avatar2"><image src="{{userdata.avatar}}" lazy-load="{{true}}" mode="scaleToFill" data-event-opts="{{[['error',[['imageError',['$event']]]]]}}" binderror="__e"></image></view></block></view><view class="box"><block wx:if="{{!islogin}}"><view class="box1"><view class="box1-text">您尚未登陆</view><view data-event-opts="{{[['tap',[['handleIsLogin',['$event']]]]]}}" class="box1-btn" bindtap="__e">点击登录</view></view></block><block wx:if="{{islogin}}"><view class="box2"><view class="box2-name"><text>{{userdata.area}}</text><text>{{userdata.name}}</text></view><view class="box2-img"><image src="{{$root.m0}}" lazy-load="{{true}}" mode="scaleToFill" data-event-opts="{{[['error',[['imageError',['$event']]]]]}}" binderror="__e"></image></view><view class="box2-rank">{{''+userdata.flexible_SET_of_row+''}}</view></view></block></view></view><button class="forward" open-type="share">转发给微信好友</button></view></view>