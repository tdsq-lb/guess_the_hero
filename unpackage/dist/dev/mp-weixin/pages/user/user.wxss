page {
  height: 100%;
}
.content {
  width: 100%;
  height: 100%;
  background-image: url(data:image/jpeg;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  padding: 40rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
}
.content .title {
  position: absolute;
  right: 40rpx;
  top: 0;
  color: #FFFFFF;
  font-size: 26rpx;
}
.content .head {
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
}
.content .head .head_avatar {
  width: 170rpx;
  height: 170rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  border: 4rpx solid #FFFFFF;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.content .info {
  width: 100%;
  height: 100rpx;
}
.content .info text {
  display: block;
  color: #FFFFFF;
  text-align: center;
  font-size: 30rpx;
  margin: 20rpx 0;
}
.content .info .name {
  font-size: 35rpx;
}

