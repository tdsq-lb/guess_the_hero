<view class="imt-audio"><view class="audio-wrapper"><slider class="audio-slider" activeColor="{{color}}" disabled="{{true}}" block-size="10" value="{{current}}" max="{{100}}"></slider></view><view class="audio-control-wrapper" style="{{'color:'+(color)+';'}}"><block tt:if="{{control}}"><view data-event-opts="{{[['tap',[['prev',['$event']]]]]}}" class="audio-control audio-control-prev" style="{{'border-color:'+(color)+';'}}" bindtap="__e"></view></block><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{(('audio-control audio-control-switch')+' '+((loading)?'audioLoading':''))}}" style="{{'border-color:'+(color)+';'}}" bindtap="__e">{{loading?'':paused?'':''}}</view><block tt:if="{{control}}"><view data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="audio-control audio-control-next" style="{{'border-color:'+(color)+';'}}" bindtap="__e"></view></block></view></view>