<view class="content"><view class="banner"><image src="../../static/images/big1000.jpg" mode="scaleToFill" lazy-load="{{true}}"></image><view class="mask"><view class="{{(visible===1?'display':'mask_item left_top')}}"></view><view class="{{(visible===2?'display':'mask_item right_top')}}"></view><view class="{{(visible===3?'display':'mask_item left_bottom')}}"></view><view class="{{(visible===4?'display':'mask_item right_bottom')}}"></view></view></view><button type="default" data-event-opts="{{[['tap',[['handleVisible',['$event']]]]]}}" bindtap="__e">提示</button><view class="progress_bar"><view class="schedule" style="{{'width:'+(audioSchedule)+';'}}"></view></view><button type="default" data-event-opts="{{[['tap',[['handleAudio',['$event']]]]]}}" bindtap="__e">播放</button></view>