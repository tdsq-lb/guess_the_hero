.user {
  width: 100%;
  height: 100%;
  position: relative;
}
.user .user-bg {
  width: 100%;
  height: 992rpx;
}
.user .user-content {
  width: 100%;
  height: 800rpx;
  position: absolute;
  top: 200rpx;
  left: 0;
}
.user .user-content .user-content-image {
  width: 100%;
  height: 100%;
}
.user .user-content .reset {
  position: absolute;
  right: 10rpx;
  top: 20rpx;
  color: #c4a264;
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.user .user-bg-content {
  position: absolute;
  left: 50%;
  top: 80rpx;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
  background-image: url(https://tdsq.top/static/images/sprite-img_01.png);
  background-size: 100% 100%;
  width: 440rpx;
  height: 550rpx;
}
.user .forward {
  width: 70%;
  height: 100rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #FFFFFF;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
  position: absolute;
  left: 50%;
  bottom: 0%;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
}
.user .avatar {
  position: absolute;
  top: -90rpx;
  left: 50%;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
}
.user .avatar .avatar1 {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 360rpx;
  height: 150rpx;
}
.user .avatar .avatar2 {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 360rpx;
  height: 150rpx;
  position: relative;
}
.user .avatar .avatar2 image {
  width: 130rpx;
  height: 130rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  display: block;
  position: absolute;
  z-index: -1;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.user .box {
  width: 320rpx;
  height: 70%;
  margin: 0 auto;
  margin-top: 60rpx;
  padding: 10rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: relative;
  text-align: center;
}
.user .box .box1 {
  color: #FFFFFF;
  font-size: 30rpx;
}
.user .box .box1-text {
  margin-top: 100rpx;
}
.user .box .box1-btn {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 226rpx;
  height: 88rpx;
  line-height: 88rpx;
  margin: 80rpx auto 0;
  font-weight: bold;
}
.user .box .box2 {
  padding: 20rpx 0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.user .box .box2 .box2-name {
  color: #95a0ad;
  font-size: 35rpx;
}
.user .box .box2 .box2-name text {
  display: block;
  padding: 10rpx;
}
.user .box .box2 .box2-img {
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}
.user .box .box2 .box2-img image {
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.user .box .box2 .box2-rank {
  color: #FFFFFF;
  font-size: 35rpx;
  font-weight: bold;
}

